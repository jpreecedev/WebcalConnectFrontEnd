{
  "name": "webcalconnectfrontend",
  "version": "2.0.0",
  "scripts": {
    "test": "karma start karma.conf.js",
    "posttest": "node_modules/.bin/remap-istanbul -i coverage/report-json/coverage-final.json -o coverage -t html",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\"",
    "docker-build": "docker build -t webcalconnectfrontend .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 webcalconnectfrontend",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "gulp": "gulp",
    "typings": "typings"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.2",
    "@angular/compiler": "^2.0.0-rc.2",
    "@angular/core": "^2.0.0-rc.2",
    "@angular/http": "^2.0.0-rc.2",
    "@angular/platform-browser": "^2.0.0-rc.2",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.2",
    "@angular/router-deprecated": "^2.0.0-rc.2",
    "bootbox": "^4.4.0",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "es6-promise": "^3.2.1",
    "es6-shim": "^0.35.1",
    "font-awesome": "^4.6.1",
    "jquery": "^2.2.4",
    "moment": "^2.13.0",
    "ng2-pagination": "^0.3.4",
    "pikaday": "^1.4.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.25",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.1.0",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-change": "^1.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^2.0.6",
    "gulp-concat": "^2.6.0",
    "gulp-flatten": "^0.2.0",
    "gulp-htmlmin": "^2.0.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-inject": "^4.1.0",
    "gulp-inline-ng2-template": "^2.0.4",
    "gulp-load-plugins": "^1.2.0",
    "gulp-rev": "^7.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-strip-comments": "^2.4.3",
    "gulp-tslint": "^5.0.0",
    "gulp-typescript": "^2.13.4",
    "gulp-uglify": "^1.5.3",
    "imagemin-pngquant": "^5.0.0",
    "lite-server": "^2.2.0",
    "run-sequence": "^1.2.1",
    "tslint": "^3.10.2",
    "typescript": "^1.8.9",
    "typings": "^1.3.0"
  },
  "description": "WebcalConnect.com front end website written in Angular 2",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpreecedev/WebcalConnectFrontEnd.git"
