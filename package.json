{
  "name": "webcalconnectfrontend",
  "version": "2.0.0",
  "scripts": {
    "postinstall": "npm run typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "sass": "sass --watch ./css/site.scss ./app/**/*.scss",
    "start": "concurrent \"npm run tsc:w\" \"npm run sass\" \"npm run lite\" ",
    "typings": "typings"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.7",
    "systemjs": "0.19.20",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15",
    "bootstrap": "3.3.6",
    "jquery": "2.2.1",
    "font-awesome": "4.5.0",
    "ng2-pagination": "^0.0.1-beta.1"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "gulp": "3.9.1",
    "gulp-sass": "^2.2.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.7.5",
    "typings": "^0.6.8"
  }
}
