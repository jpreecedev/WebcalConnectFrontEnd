{
  "compilerOptions": {
    "target": "es5",
    "module": "system",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "removeComments": false,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "outFile": "lib/app.js"
  },
  "exclude": [
    "node_modules",
    "test",
    "typings",
    "wwwroot"
  ]
}